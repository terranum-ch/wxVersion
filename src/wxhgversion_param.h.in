#ifndef _WXHGVERSION_PARAM_H_
#define _WXHGVERSION_PARAM_H_

#include "wx/wxprec.h"
#ifndef WX_PRECOMP
#include <wx/wx.h>
#endif

// USING WXHGVERSION PLUGIN 
#cmakedefine WXHGVERSION_ENABLED

// SOFTWARE INFOS
#cmakedefine WXHGVERSION_SOFT_NAME "@WXHGVERSION_SOFT_NAME@"
#cmakedefine WXHGVERSION_SOFT_TEXT "@WXHGVERSION_SOFT_TEXT@"
#cmakedefine WXHGVERSION_SOFT_BRANCH "@WXHGVERSION_SOFT_BRANCH@"
#cmakedefine WXHGVERSION_SOFT_NUMBER "@WXHGVERSION_SOFT_NUMBER@"

// WXHGSOFTWARE INFOS
#cmakedefine WXHGVERSION_NAME "@WXHGVERSION_NAME@"
#cmakedefine WXHGVERSION_TEXT "@WXHGVERSION_TEXT@"
#cmakedefine WXHGVERSION_BRANCH "@WXHGVERSION_BRANCH@"
#cmakedefine WXHGVERSION_NUMBER "@WXHGVERSION_NUMBER@"

// WXWIDGETS version
#cmakedefine WXHGVERSION_WXWIDGETS_SVN "@WXHGVERSION_WXWIDGETS_SVN@"

// VROOMGIS
#cmakedefine VROOMGIS_PATH
#ifdef VROOMGIS_PATH
    #cmakedefine VROOMGIS_TEXT "@VROOMGIS_TEXT@"
    #cmakedefine VROOMGIS_BRANCH "@VROOMGIS_BRANCH@"
    #cmakedefine VROOMGIS_NUMBER "@VROOMGIS_NUMBER@"
#endif

// GDAL
#cmakedefine GDAL_INCLUDE_DIR
#ifdef GDAL_INCLUDE_DIR
    #include <gdal_priv.h>
#endif

// GEOS
#cmakedefine GEOS_INCLUDE_DIR
#ifdef GEOS_INCLUDE_DIR
    #include <geos_c.h>
#endif

// CURL
#cmakedefine CURL_INCLUDE_DIR
#ifdef CURL_INCLUDE_DIR
    #include <curl/curl.h>
#endif

// SQLITE 3
#cmakedefine SQLITE_LIBRARIES
#ifdef SQLITE_LIBRARIES
    #include <sqlite3.h>
#endif

// MYSQL 
#cmakedefine MYSQL_INCLUDE_DIR
#ifdef MYSQL_INCLUDE_DIR
    #include <mysql.h>
#endif

// NETCDF 
#cmakedefine NetCDF_LIBRARIES
#ifdef NetCDF_LIBRARIES
    #include <netcdf.h>
#endif

// PROJ 
#cmakedefine PROJ_LIBRARY
#ifdef PROJ_LIBRARY
    #include <proj_api.h>
#endif
#cmakedefine PROJ4_INCLUDE_DIR
#ifdef PROJ4_INCLUDE_DIR
    #include <proj_api.h>
#endif

// EIGEN 
#cmakedefine EIGEN_INCLUDE_DIR
#ifdef EIGEN_INCLUDE_DIR
    #include <Eigen/Core>
#endif


#endif
